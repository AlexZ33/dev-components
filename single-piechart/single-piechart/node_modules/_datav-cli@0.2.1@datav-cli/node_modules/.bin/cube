#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_node-cube@2.0.18@node-cube/bin/cube" "$@"
  ret=$?
else 
  node  "$basedir/../../../_node-cube@2.0.18@node-cube/bin/cube" "$@"
  ret=$?
fi
exit $ret
